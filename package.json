{
  "name": "@storybook/addon-devkit",
  "version": "1.1.1",
  "description": "Storybook Addon Development Kit",
  "author": {
    "name": "Oleg Proskurin",
    "url": "https://github.com/UsulPro"
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "prepare-storybook": "yarn prepare-dev && yarn prepare && start-storybook -p 9001 --ci",
    "start-storybook": "start-storybook -p 9001 --ci",
    "start": "nodemon --exec yarn start-storybook",
    "build-storybook": "build-storybook -s public",
    "dev": "node .scripts/npm-prepare.js --watch",
    "prepare-dev": "babel dev --out-dir dev-dist --verbose",
    "prepare": "package-prepare",
    "postpublish": "node .scripts/npm-postpublish.js"
  },
  "dependencies": {
    "@reach/rect": "^0.2.1",
    "@storybook/addons": "^5.0.11",
    "@storybook/core-events": "^5.0.11",
    "@storybook/theming": "^5.0.11",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-backgrounds": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@usulpro/package-prepare": "^1.1.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.2",
    "nodemon": "^1.18.9",
    "prettier": "^1.18.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "3.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sm-react/storybook-adk"
  },
  "keywords": [
    "storybook",
    "react",
    "addon",
    "decorator",
    "customization",
    "boilerplate",
    "npm",
    "development",
    "addons"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sm-react/storybook-adk/issues"
  },
  "homepage": "https://github.com/sm-react/storybook-adk",
  "peerDependencies": {
    "@storybook/addons": "^5.0.0",
    "@storybook/react": "^5.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
